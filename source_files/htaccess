RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(\w{2,3}(?:-\w{2}(?:-mac)?)?/)?(firefox|mobile)/(\d+)\.0/releasenotes/$ $1$2/$3.0beta/releasenotes/ [PT]


